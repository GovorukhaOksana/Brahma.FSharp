{
open TTA.ASM
}

tokens {
    _ of string
}

options {
    translate = true
    module = "Parser.Parser"
    infEpsPath = epsilons
    pos = uint64
}

module Main

[<Start>]
program: (stmt NL)+ stmt

stmt: (e=operation SEMI)+

operation: EPS {Eps} | SET ln=NUMBER col=NUMBER arg=NUMBER {Set((int ln, int col), int arg)} | MVC ln=NUMBER col=NUMBER arg=NUMBER {Mvc((int ln, int col), int arg)} | MOV ln1=NUMBER col1=NUMBER ln2=NUMBER col2=NUMBER {Mov((int ln1, int col1), (int ln2, int col2))}
